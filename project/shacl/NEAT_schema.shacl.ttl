<https://w3id.org/neat_schema/:NodeEmbeddingsParams> a sh:NodeShape ;
    sh:closed true ;
    sh:description "Node embeddings parameters." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Size of the context and target nodes for node2vec." ;
            sh:maxCount 1 ;
            sh:order 3 ;
            sh:path <https://w3id.org/neat_schema/:window_size> ],
        [ sh:description "Name of the node embedding method." ;
            sh:in ( "SkipGram" "CBOW" "GloVe" "Siamese" "TransE" "SimplE" "TransH" "TransR" ) ;
            sh:maxCount 1 ;
            sh:order 0 ;
            sh:path <https://w3id.org/neat_schema/:method_name> ],
        [ sh:description "Number of iterations." ;
            sh:maxCount 1 ;
            sh:order 6 ;
            sh:path <https://w3id.org/neat_schema/:iterations> ],
        [ sh:description "Weight on the probability of returning to the same node the walk just came from Having this higher tends the walks to be more like a Breadth-First Search. Having this very high  (> 2) makes search very local. Equal to the inverse of p in the Node2Vec paper." ;
            sh:maxCount 1 ;
            sh:order 4 ;
            sh:path <https://w3id.org/neat_schema/:return_weight> ],
        [ sh:description "Weight on the probability of visiting a neighbor node to the one we're coming from in the random walk Having this higher tends the walks to be more like a Depth-First Search. Having this very high makes search more outward. Having this very low makes search very local. Equal to the inverse of q in the Node2Vec paper." ;
            sh:maxCount 1 ;
            sh:order 5 ;
            sh:path <https://w3id.org/neat_schema/:explore_weight> ],
        [ sh:description "Maximal length of the walks." ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path <https://w3id.org/neat_schema/:walk_length> ],
        [ sh:description "Number of nodes to include in a single batch." ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path <https://w3id.org/neat_schema/:batch_size> ] ;
    sh:targetClass <https://w3id.org/neat_schema/:NodeEmbeddingsParams> .

